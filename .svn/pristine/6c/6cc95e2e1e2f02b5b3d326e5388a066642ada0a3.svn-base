<!DOCTYPE HTML>
<html>
<head>
	<meta charset="utf-8">
	<meta name="renderer" content="webkit|ie-comp|ie-stand">
	<meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
	<meta name="viewport"
		  content="width=device-width,initial-scale=1,minimum-scale=1.0,maximum-scale=1.0,gprs-scalable=no" />
	<meta http-equiv="Cache-Control" content="no-siteapp" />
	<!--[if lt IE 9]>
	<script type="text/javascript" src="{$responseAdmin}/lib/html5.js"></script>
	<script type="text/javascript" src="{$responseAdmin}/lib/respond.min.js"></script>
	<script type="text/javascript" src="{$responseAdmin}/lib/PIE_IE678.js"></script>
	<![endif]-->
	<link href="{$responseAdmin}/css/H-ui.min.css" rel="stylesheet"
		  type="text/css" />
	<link href="{$responseAdmin}/css/H-ui.admin.css" rel="stylesheet"
		  type="text/css" />
	<link href="{$responseAdmin}/lib/icheck/icheck.css" rel="stylesheet"
		  type="text/css" />
	<link href="{$responseAdmin}/lib/Hui-iconfont/1.0.1/iconfont.css"
		  rel="stylesheet" type="text/css" />
	<!--[if IE 6]>
	<script type="text/javascript" src="http://lib.h-ui.net/DD_belatedPNG_0.0.8a-min.js" ></script>
	<script>DD_belatedPNG.fix('*');</script>
	<![endif]-->
	<title>添加等级</title>
</head>
<body>
<div class="pd-20">

	<form action="" method="post" class="form form-horizontal" id="form-vipInfo-add">
		<div class="row cl">
			<label class="form-label col-1"><span class="c-red">*</span>商品标识：</label>
			<div class="formControls col-1">
				<input type="text" class="input-text" value="{$info['Code']}" placeholder="请输入商品标识"
					   name="Code" datatype="*1-16" nullmsg="商品标识不能为空">
				<input type="hidden" class="input-text" value="{$info['Id']}"
					   name="id"  >
			</div>
			<div class="col-1"></div>
			<label class="form-label col-1"><span class="c-red">*</span>商品名：</label>
			<div class="formControls col-1">
				<input type="text" class="input-text" value="{$info['Name']}" placeholder="请输入商品名"
					   name="Name" datatype="*1-16" nullmsg="商品名不能为空">
			</div>
			<div class="col-1"></div>
			<label class="form-label col-1"><span class="c-red">*</span>价格：</label>
			<div class="formControls col-1">
				<input type="text" class="input-text" value="{$info['CurrencyNum']}" placeholder="请输入价格"
					   name="CurrencyNum" datatype="*1-20" nullmsg="价格不能为空">
			</div>
			<div class="col-1"></div><label class="form-label col-1">
			<span class="c-red">*</span>购买货币：</label>
			<div class="formControls col-1">
				<div style="width:100%;text-align:center;float:left">
		        		<span class="select-box">
							<select  class="select" name = "CurrencyType"   nullmsg="" >
								 <option value="1" <eq name="info['CurrencyType']" value = "1">selected="selected"</eq> >人民币</option>
								 <option value="2" <eq name="info['CurrencyType']" value = "2">selected="selected"</eq> >金币</option>
								 <option value="3" <eq name="info['CurrencyType']" value = "3">selected="selected"</eq>>砖石</option>
                            </select>
						</span>

				</div>
			</div>

		</div>
		<div class="row cl">
			<label class="form-label col-1"><span class="c-red">*</span>是否限制发行量：</label>
			<div class="formControls col-1">
				<div style="width:100%;text-align:center;float:left">
		        		<span class="select-box">
							<select  class="select" name = "IssueType"   onchange="IssueTypeFun(this)"  nullmsg="" >
								 <option value="1"  <eq name="info['IssueType']" value = "1">selected="selected"</eq>>否</option>
								 <option value="2" <eq name="info['IssueType']" value = "2">selected="selected"</eq>>是</option>
                            </select>
						</span>
				</div>
			</div>
			<div class="formControls col-1 " >
				<input type="text"  id = "IssueNum" class="input-text"   <eq name="info['IssueType']" value = "1">style="display: none;"</eq> value="{$info['IssueNum']}" placeholder=""
					   name="IssueNum"  nullmsg="充值额度不能为空">
			</div>
			<label class="form-label col-1"><span class="c-red">*</span>类别：</label>
			<div class="formControls col-1">
				<div style="width:100%;text-align:center;float:left">
		        		<span class="select-box">
							<select  class="select" name = "CateGory"   nullmsg="" >
								 <option value="1" 	<eq name="info['CateGory']" value = "1">selected="selected"</eq> 	>金币</option>
								 <option value="2"  <eq name="info['CateGory']" value = "2">selected="selected"</eq>	>钻石</option>
								 <option value="3"  <eq name="info['CateGory']" value = "3">selected="selected"</eq>	>道具</option>
                            </select>
						</span>
				</div>
			</div>
			<label class="form-label col-2"><span class="c-red">*</span>类型：</label>
			<div class="formControls col-1">
				<div style="width:100%;text-align:center;float:left">
		        		<span class="select-box">
							<select  class="select" name = "Type"   nullmsg="" >
								 <option value="1"  <eq name="info['Type']" value = "1">selected="selected"</eq> >金币</option>
								 <option value="2"  <eq name="info['Type']" value = "2">selected="selected"</eq> >钻石</option>
								 <option value="3"  <eq name="info['Type']" value = "3">selected="selected"</eq>>道具</option>
                            </select>
						</span>
				</div>
			</div>
			<label class="form-label col-2"><span class="c-red">*</span>展示方式：</label>
			<div class="formControls col-1">
				<div style="width:100%;text-align:center;float:left">
		        		<span class="select-box">
							<select  class="select" name = "ShowType"  nullmsg="" >
								 <option value="1"  <eq name="info['ShowType']" value = "1">selected="selected"</eq> >商城</option>
                            </select>
						</span>
				</div>
			</div>
		</div>
		<div class="row cl">
			<label class="form-label col-1"><span class="c-red">*</span>获得数量：</label>
			<div class="formControls col-1">
				<input type="text" class="input-text" value="{$info['GetNum']}" placeholder="请输入数量"
					   name="GetNum" datatype="*1-20" nullmsg="获得数量不能为空">
			</div>
			<div class="col-1"></div>

			<label class="form-label col-1"><span class="c-red">*</span>平台：</label>
			<div class="formControls col-1">
				<div style="width:100%;text-align:center;float:left">
		        		<span class="select-box">
							<select  class="select" name = "Platform"   nullmsg="" >
								 <option value="1"  <eq name="info['Platform']" value = "1">selected="selected"</eq>  >全部</option>
								 <option value="2"  <eq name="info['Platform']" value = "2">selected="selected"</eq> >苹果</option>
								 <option value="3"   <eq name="info['Platform']" value = "3">selected="selected"</eq> >安卓</option>
                            </select>
						</span>
				</div>
			</div>
			<label class="form-label col-2"><span class="c-red">*</span>等级限制：</label>
			<div class="formControls col-1">
				<div style="width:100%;text-align:center;float:left">
		        		<span class="select-box">
							<select  class="select" name = "LimitLevel"   nullmsg="" >
								 <option value="0" <eq name="info['LimitLevel']" value = "0">selected="selected"</eq> >不限</option>
								 <option value="1" <eq name="info['LimitLevel']" value = "1">selected="selected"</eq> >1</option>
								 <option value="2" <eq name="info['LimitLevel']" value = "2">selected="selected"</eq> >2</option>
                            </select>
						</span>
				</div>
			</div>
			<label class="form-label col-2"><span class="c-red">*</span>vip限制：</label>
			<div class="formControls col-1">
				<div style="width:100%;text-align:center;float:left">
		        		<span class="select-box">
							<select  class="select" name = "LimitVip"   nullmsg="" >
								 <option value="0" <eq name="info['LimitVip']" value = "0">selected="selected"</eq> >不限</option>
								 <option value="1" <eq name="info['LimitVip']" value = "1">selected="selected"</eq> >1</option>
								 <option value="2" <eq name="info['LimitVip']" value = "2">selected="selected"</eq> >2</option>
                            </select>
						</span>
				</div>
			</div>
		</div>

		<div class="row cl">
			<label class="form-label col-1"><span class="c-red">*</span>限购：</label>
			<div class="formControls col-1">
				<div style="width:100%;text-align:center;float:left">
		        		<span class="select-box">
							<select  class="select" name = "LimitShop"  onchange="LimitShopFun(this)" nullmsg="" >
								 <option value="1" <eq name="info['LimitShop']" value = "1">selected="selected"</eq> >不限</option>
								 <option value="2" <eq name="info['LimitShop']" value = "2">selected="selected"</eq> >日</option>
								 <option value="3" <eq name="info['LimitShop']" value = "3">selected="selected"</eq> >周</option>
								 <option value="4" <eq name="info['LimitShop']" value = "4">selected="selected"</eq>>月</option>
                            </select>
						</span>
				</div>
			</div>
			<div class="formControls col-1">
				<input type="text"  class="input-text" <eq name="info['LimitShop']" value = "1">style="display: none;"</eq>  id="LimitShopNum" value="{$info['LimitShopNum']}" placeholder="请输入数量"
					   name="LimitShopNum" >
			</div>

			<label class="form-label col-1"><span class="c-red">*</span>限兑：</label>

			<div class="formControls col-1">
				<div style="width:100%;text-align:center;float:left">
		        		<span class="select-box">
							<select  class="select" name = "ExchangeType"  onchange="ExchangeTypeFun(this)"  nullmsg="" >
								 <option value="1" <eq name="info['ExchangeType']" value = "1">selected="selected"</eq> >不限</option>
								<option value="2"  <eq name="info['ExchangeType']" value = "2">selected="selected"</eq> >日</option>
								<option value="3"  <eq name="info['ExchangeType']" value = "3">selected="selected"</eq> >周</option>
								<option value="4"  <eq name="info['ExchangeType']" value = "4">selected="selected"</eq>>月</option>
                            </select>
						</span>
				</div>
			</div>
			<div class="formControls col-1">
				<input type="text"  class="input-text" <eq name="info['ExchangeType']" value = "1">style="display: none;"</eq> value="{$info['ExchangeNum']}" placeholder="请输入数量"
					   name="ExchangeNum" id ="ExchangeNum"  nullmsg="充值额度不能为空">
			</div>
			<label class="form-label col-1"><span class="c-red">*</span>状态：</label>
			<div class="formControls col-1">
				<div style="width:100%;text-align:center;float:left">
		        		<span class="select-box">
							<select  class="select" name = "Status"  onchange="StatusFun(this)" nullmsg="" >
								 <option value="1" <eq name="info['Status']" value = "1">selected="selected"</eq> >立即上架</option>
								 <option value="2" <eq name="info['Status']" value = "2">selected="selected"</eq>>定时上架</option>
								 <option value="3" <eq name="info['Status']" value = "3">selected="selected"</eq>>定时下架</option>
								 <option value="4" <eq name="info['Status']" value = "4">selected="selected"</eq>>立即下架</option>
                            </select>
						</span>
				</div>
			</div>
			<div class="formControls col-1"  style="display: none;">
				<input type="hidden"  onfocus="<literal>WdatePicker({maxDate:'#F{$dp.$D(\'datemax\')||\'%y-%M-%d\'}'})</literal>"
					   id="datemin" value="{$datemin}" name="datemin" class="input-text Wdate"    style="width: 120px;">
			</div>
			<div class="formControls col-1" >
				<input type="text"
					   onfocus="<literal>WdatePicker({minDate:'#F{$dp.$D(\'datemin\')}',maxDate:''})</literal>"
					   id="TheShelvesTime"    value="{$info['TheShelvesTime']|date='Y-m-d',###}"     placeholder = "请选择时间" name="TheShelvesTime"
					   class="input-text Wdate"  <eq name="info['Status']" value = "1">style="display:none;"<elseif  condition = "$info['Status'] eq 4" />style="display:none;"</eq>
						>
			</div>
			<label class="form-label col-1"><span class="c-red">*</span>首冲比例/百分比：</label>
			<div class="formControls col-1">
				<input type="text" class="input-text" value="{$info['Proportion']}" ="请输入整数"
					   name="Proportion" datatype="*1-20" nullmsg="首冲比例不能为空">
			</div>
		</div>

		<div class="row cl">
			<label class="form-label col-1"><span class="c-red">*</span>是否赠送：</label>
			<div class="formControls col-1">
				<div style="width:100%;text-align:center;float:left">
		        		<span class="select-box">
							<select  class="select" name = "IsGive"  onchange="GiveInfoFun(this)" nullmsg="" >
								<option value="1" <empty name = "info['GiveInfo']">selected="selected"</empty> >否</option>
								 <option value="2" <notempty name = "info['GiveInfo']">selected="selected"</notempty>>是</option>

                            </select>
						</span>
				</div>
			</div>
			<div class="col-1"></div>
			<div id = "operation"  <empty name = "info['GiveInfo']">style="display: none;</empty> ">
				<div class="formControls col-0"    onclick="GiveInfoAdd()">
					<input class="btn btn-primary radius" type="button"  value="&nbsp;&nbsp;增加&nbsp;&nbsp;">

				</div>
				<div   id = "del" class="formControls col-0">
					<input class="btn btn-primary radius" type="button" onclick="GiveInfoDel()" value="&nbsp;&nbsp;删除&nbsp;&nbsp;">

				</div>
			</div>
		</div>
		<div id = "parent" >
			<div class="row cl GiveInfo"  <empty name = "info['GiveInfo']">style="display: none"</empty> >
				<label class="form-label col-1"><span class="c-red"></span></label>
				<div class="formControls col-1">
					<div style="width:100%;text-align:center;float:left">
		        		<span class="select-box">
							<select  class="select GoodsType"  name = "GiveInfo[0][Type]"  onchange="selectGoodsFun(this)" nullmsg="" >
								 <option value="0">请选择类型</option>
								<option <eq name = "info['GiveInfo'][0]['Type']" value ="1">selected="selected"</eq> value="1">金币</option>
								 <option  <eq name = "info['GiveInfo'][0]['Type']" value ="2">selected="selected"</eq> value="2">砖石</option>

                            </select>
						</span>
					</div>
				</div>
				<div class="formControls col-1">
					<div style="width:100%;text-align:center;float:left">
		        		<span class="select-box">
							<select  class="select Id" name = "GiveInfo[0][Id]"  nullmsg="" >
								 <option value="0">请选择商品</option>
								<volist name = "GoodsAllList" id="val">
									<option class="type{$val['Type']}" value="{$val['Id']}"
										<if condition = "$info['GiveInfo'][0]['Id'] eq $val['Id']">selected="selected"</if>
										<if condition = "$info['GiveInfo'][0]['Type'] neq $val['Type']">style="display: none;"</if> >
										{$val['Name']}
									</option>
								</volist>
                            </select>
						</span>
					</div>
				</div>
				<div class="formControls col-1">
					<input type="text"  class="input-text GetNum"   value="{$info['GiveInfo'][0]['GetNum']}" placeholder="请输入数量"
						   name="GiveInfo[0][GetNum]"  nullmsg="充值额度不能为空">
				</div>
			</div>
		</div>
		<div id = "child">
				<volist name = "info['GiveInfo']"  id = "v" key = "k">
					<if condition = "$k gt 1">
							<div class="row cl GiveInfo"   >
							<label class="form-label col-1"><span class="c-red"></span></label>
							<div class="formControls col-1">
								<div style="width:100%;text-align:center;float:left">
								<span class="select-box">
									<select  class="select GoodsType"  name = "GiveInfo[{$k}][Type]"  onchange="selectGoodsFun(this)" nullmsg="" >
										 <option value="0">请选择类型</option>
										<option <eq name = "v['Type']"   value ="1">selected="selected"</eq> value="1">金币</option>
										<option  <eq name = "v['Type']"  value ="2">selected="selected"</eq> value="2">砖石</option>

									</select>
								</span>
								</div>
							</div>
							<div class="formControls col-1">
								<div style="width:100%;text-align:center;float:left">
							<span class="select-box">
								<select  class="select Id" name = "GiveInfo[{$k}][Id]"  nullmsg="" >
									 <option value="0">请选择商品</option>
									<volist name = "GoodsAllList" id="val">
										<option class="type{$val['Type']}" <if condition = "$v['Id'] eq $val['Id']">selected="selected"</if> <if condition = "$v['Type'] neq $val['Type']">style="display: none;"</if> value="{$val['Id']}">
											{$val['Name']}
										</option>
									</volist>
								</select>
							</span>
								</div>
							</div>
							<div class="formControls col-1">
								<input type="text"  class="input-text GetNum"   value="{$v['GetNum']}" placeholder="请输入数量"
									   name="GiveInfo[{$k}][GetNum]"  nullmsg="充值额度不能为空">
							</div>
						</div>
					</if>
				</volist>
		</div>
		<div class="row cl">
			<label class="form-label col-1">商品描述：</label>
			<div class="formControls col-10">
				<textarea name="Describe" cols="" rows="" class="textarea"  placeholder="请输入商品描述" datatype="*0-100" dragonfly="true" nullmsg="备注不能为空！" onKeyUp="textarealength(this,200)">{$info['Describe']}</textarea>
				<p class="textarea-numberbar"><em class="textarea-length">0</em>/200</p>
			</div>
		</div>
		<div class="row cl">
			<label class="form-label col-1">广播信息：</label>
			<div class="formControls col-10">
				<textarea name="Broadcast" cols="" rows="" class="textarea"  placeholder="请输入广播信息" datatype="*0-100" dragonfly="true" nullmsg="备注不能为空！" onKeyUp="textarealength(this,200)">{$info['Broadcast']}</textarea>
				<p class="textarea-numberbar"><em class="textarea-length">0</em>/200</p>
			</div>
		</div>
		<div class="row cl">
			<label class="form-label col-1">推送信息：</label>
			<div class="formControls col-10">
				<textarea name="Push" cols="" rows="" class="textarea"  placeholder="请输入推送信息" datatype="*0-100" dragonfly="true" nullmsg="备注不能为空！" onKeyUp="textarealength(this,200)">{$info['Push']}</textarea>
				<p class="textarea-numberbar"><em class="textarea-length">0</em>/200</p>
			</div>
		</div>
		<div class="row cl">
			<label class="form-label col-1">备注：</label>
			<div class="formControls col-10">
				<textarea name="Rmark" cols="" rows="" class="textarea"  placeholder="请输入备注" datatype="*0-100" dragonfly="true" nullmsg="备注不能为空！" onKeyUp="textarealength(this,200)">{$info['Rmark']}</textarea>
				<p class="textarea-numberbar"><em class="textarea-length">0</em>/200</p>
			</div>
		</div>
		<div class="row cl">
			<div class="col-9 col-offset-3">
				<input class="btn btn-primary radius" type="submit" name="submit" value="&nbsp;&nbsp;提交&nbsp;&nbsp;">
			</div>
		</div>
	</form>
</div>

<script type="text/javascript"
		src="{$responseAdmin}/lib/jquery/1.9.1/jquery.min.js"></script>
<script type="text/javascript"
		src="{$responseAdmin}/lib/icheck/jquery.icheck.min.js"></script>
<script type="text/javascript"
		src="{$responseAdmin}/lib/Validform/5.3.2/Validform.min.js"></script>
<script type="text/javascript"
		src="{$responseAdmin}/lib/layer/1.9.3/layer.js"></script>
<script type="text/javascript" src="{$responseAdmin}/lib/My97DatePicker/WdatePicker.js"></script>
<script type="text/javascript" src="{$responseAdmin}/lib/datatables/1.10.0/jquery.dataTables.min.js"></script>
<script type="text/javascript" src="{$responseAdmin}/js/H-ui.js"></script>
<script type="text/javascript" src="{$responseAdmin}/js/H-ui.admin.js"></script>

<literal>
	<script type="text/javascript">
        $(function() {
            $('.skin-minimal input').iCheck({
                checkboxClass : 'icheckbox-blue',
                radioClass : 'iradio-blue',
                increaseArea : '20%'
            });

            $("#form-vipInfo-add").Validform({
                tiptype : 2,

                callback : function(form) {
                    form[0].submit();
                    var index = parent.layer.getFrameIndex(window.name);
                    parent.$('.btn-refresh').click();
                    parent.layer.close(index);
                }
            });


        });

	</script>
	<script type="text/javascript">
        //是否限制发行量
        function IssueTypeFun(my) {

            var Index 		= my.selectedIndex;         				//索引
            var Value 		= my.options[Index].value;   				//选择中值
            var IssueNum	= document.getElementById('IssueNum');		//发行数量
            if(Value == 1){
                IssueNum.style.display = "none";
            }else{
                IssueNum.style.display = "block";
            }

        }
        //限购

        function LimitShopFun(my) {
            var Index 			= my.selectedIndex;         				//索引
            var Value 			= my.options[Index].value;   				//选择中值
            var LimitShopNum	= document.getElementById('LimitShopNum');		//限购数
            if(Value == 1){
                LimitShopNum.style.display = "none";
            }else{
                LimitShopNum.style.display = "block";
            }
        }
        //限兑
        function ExchangeTypeFun(my) {
            var Index 		= my.selectedIndex;         				//索引
            var Value 		= my.options[Index].value;   				//选择中值
            var ExchangeNum	= document.getElementById('ExchangeNum');		//限兑
            if(Value == 1){
                ExchangeNum.style.display = "none";
            }else{
                ExchangeNum.style.display = "block";
            }
        }

        //状态
        function StatusFun(my) {
            var Index 		= my.selectedIndex;         				//索引
            var Value 		= my.options[Index].value;   				//选择中值
            var TheShelvesTime	= document.getElementById('TheShelvesTime');		//限兑
            if(Value == 1 || Value == 4){
                TheShelvesTime.style.display = "none";
            }else{
                TheShelvesTime.style.display = "block";
            }
        }

        //赠送
        function GiveInfoFun(my) {
            var Index 		= my.selectedIndex;         						//索引
            var Value 		= my.options[Index].value;   						//选择中值
            var operation	= document.getElementById('operation');				//操作
            var GiveInfo	= document.getElementsByClassName('GiveInfo');		//赠送

            if(Value == 1 ){
                operation.style.display = "none";
                for(i = 0 ;i<GiveInfo.length;i++){
                    GiveInfo[i].style.display = "none";
                }

            }else{
                operation.style.display = "block";
                for(i = 0 ;i<GiveInfo.length;i++){
                    GiveInfo[i].style.display = "block";
                }
            }
        }
        //赠送 增加
        function GiveInfoAdd() {
            var  parent = document.getElementById('parent');
            var  child  = document.getElementById('child');
            var  GiveInfo = document.getElementsByClassName('GiveInfo');
            var  del      = document.getElementById('del');
            child.innerHTML +=  parent.innerHTML;

            if(GiveInfo.length == 2){
                del.style.display = "block";
            }
            var Id 			= document.getElementsByClassName('Id');
            var GetNum 			= document.getElementsByClassName('GetNum');
            var GoodsType 			= document.getElementsByClassName('GoodsType');
            var Length			=  Id.length;
            GoodsType[Length-1].setAttribute('name','GiveInfo['+Length+'][Type]');
            GetNum[Length-1].setAttribute('name','GiveInfo['+Length+'][GetNum]');
            Id[Length-1].setAttribute('name','GiveInfo['+Length+'][Id]');
            console.log(Length);


        }
        //赠送 减少
        function GiveInfoDel() {
            var  child  = document.getElementById('child');
            var  GiveInfo = document.getElementsByClassName('GiveInfo');
            var  del      = document.getElementById('del');
            var length	  = GiveInfo.length;
            if(length>1){
                child.removeChild(GiveInfo[length-1])
            }

            if(length == 2){
                del.style.display = "none";
            }
        }
        var  GiveInfo = document.getElementsByClassName('GiveInfo');
        var  del      = document.getElementById('del');
        if(GiveInfo.length<=1){
            del.style.display = "none";
		}

        //选择商品
        function  selectGoodsFun(my) {
            var GoodsType 			= document.getElementsByClassName('GoodsType')
            var Id 					= document.getElementsByClassName('Id');
            for(i=0;i<GoodsType.length;i++){
                GoodsType[i].index = i;
            }
            var MyIndex 			= my.index;
            var IdSelect 			= Id[MyIndex];
            var seleIndex           = my.selectedIndex;
            var selectValue			= my.options[seleIndex].value;
            var selectOptions       = my.options;


            var IdSelectOptionType 		= IdSelect.getElementsByClassName('type'+selectValue);
            var IdSelectOption 		= IdSelect.options;

            console.log(MyIndex);
            for(j = 0;j<IdSelectOption.length;j++){
                if(j>0){
                    IdSelectOption[j].style.display = "none";
                }
            }
            for(k=0;k<IdSelectOptionType.length;k++){
                IdSelectOptionType[k].style.display = "inline-block";
            }

        }
	</script>
